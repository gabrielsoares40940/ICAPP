{
  "expo": {
    "name": "ICAPP",
    "slug": "ICAPP",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/imaculada-icon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#63c2d1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/imaculada-icon.jpeg",
        "backgroundColor": "#63c2d1"
      },
      "package": "com.gabriel.soares.br.ICAPP"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1c31743a-1237-4a28-939c-a70612be3683"
      }
    }
  }
}
